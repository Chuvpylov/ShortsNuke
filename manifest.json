{
  "manifest_version": 3,
  "name": "YouTube Shorts Blocker",
  "version": "1.1",
  "description": "Blocks YouTube Shorts elements from appearing",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
